{% set arr = [] %}
{% for item in items %}
{%
    set arr = arr|merge([{
        content_title: item.title,
        content_url: render_var(item.url)
    }])
%}
{% endfor %}
<div class="grid-cell grid-cell--content">
{% include "@molecules/sidebar-menu.twig" with {
    "sidebar_menulist": arr
} %}
</div>